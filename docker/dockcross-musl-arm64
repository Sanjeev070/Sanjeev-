#!/usr/bin/env bash

DEFAULT_DOCKCROSS_IMAGE=dockcross/linux-arm64-musl:latest

source $( cd "$( dirname "$0" )" >/dev/null && pwd )/dockcross-source

################################################################################
#
# This image is not intended to be run manually.
#
# To create a dockcross helper script for the
# dockcross/linux-arm64-musl:latest image, run:
#
# docker run --rm dockcross/linux-arm64-musl:latest > dockcross-linux-arm64-musl-latest
# chmod +x dockcross-linux-arm64-musl-latest
#
# You may then wish to move the dockcross script to your PATH.
#
################################################################################
